<div>
<%= form_tag("/applications", method: :post) do %>

<p><%= label_tag :name %>
  <%= text_field_tag :name %></p>

  <p><%= label_tag :address %>
  <%= text_field_tag :address %></p>

  <p><%= label_tag :city %>
  <%= text_field_tag :city %></p>

  <p><%= label_tag :state %>
  <%= number_field_tag :state %></p>

  <p><%= label_tag :zip %>
  <%= text_field_tag :zip %></p>

  <p><%= label_tag :phone_number %>
  <%= text_field_tag :phone_number %></p>

  <p><%= label_tag :description %>
  <%= text_field_tag :description %></p>

    <%= submit_tag "Apply"%>
<% end %>
</div>

